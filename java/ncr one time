import java.util.*;
import java.io.*;

class Main{
    public static void main(String[] args)
    {
        Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        int r = sc.nextInt();
        int num=1;int den=1;
        for(int i=1;i<=r;i++){
            den=den*i;
            num=num*(n-i+1);
        }
        System.out.print(num/den);
    
    }

 }
