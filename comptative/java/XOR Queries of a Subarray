class Solution {
    public int[] xorQueries(int[] arr, int[][] queries) {
        int p=0;
        int a[]=new int[queries.length];
        for(int i=0;i<queries.length;i++){
            p=0;
            for(int j=queries[i][0];j<=queries[i][1];j++){
                 p=p^arr[j];
            }
            a[i]=p;
        }
       return a;     
    }
}
